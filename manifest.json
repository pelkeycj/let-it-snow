{
  "manifest_version": 2,
  "name": "Let It Snow",
  "description": "This extension replaces all appearances of 'no' and 'know' to 'snow'",
  "version": "0.1",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Let It Snow"
  },

  "permissions": [
    "tabs"
  ],

  "content_scripts": [
    {
      "js": ["jquery-3.2.1.min.js", "script.js"]
    }
  ]
}
